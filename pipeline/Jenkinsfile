pipeline {
    agent any
    stages {
        stage('Test') {
            steps {
                sh './gradlew clean build'
            }
        }
    }

    publishHTML([
    allowMissing: false,
    alwaysLinkToLastBuild: false,
    keepAll: false,
    reportDir: 'build/jacocoHtml',
    reportFiles: 'index.html',
    reportName: 'HTML Report',
    reportTitles: ''])
}